{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    // Language and Environment
    "target": "es6",
    "lib": ["dom", "dom.iterable","esnext"],
    "jsx": "react-jsx",
    // Modules
    "module": "esnext",
    // "rootDir": "src",
    "resolveJsonModule": true,
    //JavaScript Support
    "allowJs": true,
    // Interop Constraints
    "isolatedModules": true,
    // Emit
    "outDir": "dist",
  }
}
